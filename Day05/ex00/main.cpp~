#include "Bureaucrat.hpp"

int main ()
{
try
{
	Bureaucrat b1("Vasilyi", 135);
	Bureaucrat b2("Petro", 1);
	Bureaucrat b3("Sasha", 150);
	Bureaucrat b4("Stas", 42);
